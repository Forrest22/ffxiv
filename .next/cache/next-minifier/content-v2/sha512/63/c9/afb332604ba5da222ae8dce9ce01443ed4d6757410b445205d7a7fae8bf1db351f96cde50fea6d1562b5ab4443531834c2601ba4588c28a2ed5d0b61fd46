 = _useTranslation.t,\n      i18n = _useTranslation.i18n;\n\n  var locale = i18n.language;\n  return /*#__PURE__*/_jsxs(Page, {\n    title: [t('_title'), t('aboutPage._title')],\n    children: [/*#__PURE__*/_jsxs(Section, {\n      children: [/*#__PURE__*/_jsx(Typography, {\n        paragraph: true,\n        children: \"Your chocobo\\u2019s plumage can be modified by feeding it 6 possible fruits. Each fruit changes the chocobo\\u2019s RGB values according to the following table:\"\n      }), /*#__PURE__*/_jsx(Box, {\n        mb: 2,\n        children: /*#__PURE__*/_jsx(TableContainer, {\n          children: /*#__PURE__*/_jsxs(Table, {\n            size: \"small\",\n            className: classes.table,\n            children: [/*#__PURE__*/_jsx(TableHead, {\n              children: /*#__PURE__*/_jsxs(TableRow, {\n                children: [/*#__PURE__*/_jsx(TableCell, {\n                  align: \"center\",\n                  children: \"Fruit\"\n                }), /*#__PURE__*/_jsx(TableCell, {\n                  align: \"center\",\n                  children: \"R\"\n                }), /*#__PURE__*/_jsx(TableCell, {\n                  align: \"center\",\n                  children: \"G\"\n                }), /*#__PURE__*/_jsx(TableCell, {\n                  align: \"center\",\n                  children: \"B\"\n                })]\n              })\n            }), /*#__PURE__*/_jsx(TableBody, {\n              children: [Fruit.XelphatolApple, Fruit.MamookPear, Fruit.OGhomoroBerries, Fruit.DomanPlum, Fruit.Valfruit, Fruit.CieldalaesPineapple].map(function (fruit) {\n                var _fruitValues$fruit = fruitValues[fruit],\n                    R = _fruitValues$fruit.R,\n                    G = _fruitValues$fruit.G,\n                    B = _fruitValues$fruit.B;\n                return /*#__PURE__*/_jsxs(TableRow, {\n                  children: [/*#__PURE__*/_jsxs(TableCell, {\n                    children: [/*#__PURE__*/_jsx(FruitIcon, {\n                      fru