     children: bozja.drops[zone].map(function (datum) {\n                return datum.loot;\n              }).sort(function (a, b) {\n                return a - b;\n              }).filter(function (loot, index, array) {\n                return loot !== array[index + 1];\n              }).map(function (loot) {\n                return bozja.drops[zone].filter(function (datum) {\n                  return datum.loot === loot;\n                }).sort(function (a, b) {\n                  return a.monster - b.monster;\n                }).map(function (datum, index, array) {\n                  return /*#__PURE__*/_jsxs(TableRow, {\n                    children: [index === 0 && /*#__PURE__*/_jsx(TableCell, {\n                      rowSpan: array.length,\n                      align: \"center\",\n                      children: translate(locale, bozja.items[loot], 'name')\n                    }), /*#__PURE__*/_jsxs(TableCell, {\n                      align: \"center\",\n                      children: [\"\\xD7\", datum.count]\n                    }), /*#__PURE__*/_jsxs(TableCell, {\n                      align: \"center\",\n                      children: [datum.rate, \"%\"]\n                    }), /*#__PURE__*/_jsx(TableCell, {\n                      align: \"center\",\n                      children: formatMonster(translate(locale, datum, 'name'), locale)\n                    }), /*#__PURE__*/_jsx(TableCell, {\n                      align: \"center\",\n                      children: datum.condition\n                    })]\n                  }, index);\n                });\n              })\n            })]\n          }, zone);\n        })\n      })\n    })\n  });\n};\n\nexport default DropsTable;","map":null,"metadata":{},"sourceType":"module"}