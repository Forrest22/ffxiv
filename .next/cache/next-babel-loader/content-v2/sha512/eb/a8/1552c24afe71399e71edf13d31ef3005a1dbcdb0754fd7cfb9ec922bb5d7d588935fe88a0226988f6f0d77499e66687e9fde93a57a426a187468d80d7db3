{"ast":null,"code":"import { jsx as _jsx } from \"react/jsx-runtime\";\nimport React from 'react';\nimport clsx from 'clsx';\nimport { makeStyles } from '@material-ui/core/styles';\nimport IconButton from '@material-ui/core/IconButton';\nimport CheckIcon from '@material-ui/icons/Check';\nimport { ChecklistContext } from '../../pages/ocean-fishing/index';\nconst useStyles = makeStyles(() => ({\n  check: {\n    padding: 10\n  },\n  unchecked: {\n    opacity: 0.4\n  },\n  checked: {\n    color: 'green'\n  }\n}));\n\nconst ChecklistCheckmark = ({\n  fishId\n}) => {\n  const classes = useStyles();\n  return /*#__PURE__*/_jsx(ChecklistContext.Consumer, {\n    children: ({\n      checklist,\n      setChecklist\n    }) => {\n      const toggleFish = fishId => {\n        if (checklist.includes(fishId)) {\n          const copy = checklist.slice();\n          copy.splice(copy.findIndex(id => id === fishId), 1);\n          setChecklist(copy);\n        } else {\n          const copy = checklist.slice();\n          copy.push(fishId);\n          copy.sort((a, b) => a - b);\n          setChecklist(copy);\n        }\n      };\n\n      return /*#__PURE__*/_jsx(IconButton, {\n        className: clsx(classes.check, checklist.includes(fishId) ? classes.checked : classes.unchecked),\n        onClick: toggleFish.bind(null, fishId),\n        children: /*#__PURE__*/_jsx(CheckIcon, {})\n      });\n    }\n  });\n};\n\nexport default ChecklistCheckmark;","map":null,"metadata":{},"sourceType":"module"}