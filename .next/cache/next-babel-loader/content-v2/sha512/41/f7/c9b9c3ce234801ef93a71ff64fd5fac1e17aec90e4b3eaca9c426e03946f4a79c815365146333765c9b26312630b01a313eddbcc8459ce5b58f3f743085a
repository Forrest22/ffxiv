{"ast":null,"code":"import { jsx as _jsx } from \"react/jsx-runtime\";\nimport React from 'react';\nimport CardContent from '@material-ui/core/CardContent';\nimport StopCardsContainer from './StopCardsContainer';\nimport StopCard from './StopCard';\nimport BaitList from './BaitList';\nimport * as maps from './maps';\nimport { getBlueFish, getBaitGroup } from './utils';\nimport translate from '../translate';\nimport { useTranslation } from '../i18n';\n\nconst RouteInformationIntuition = ({\n  stopTimes\n}) => {\n  const {\n    i18n\n  } = useTranslation('ocean-fishing');\n  const locale = i18n.language;\n  return /*#__PURE__*/_jsx(StopCardsContainer, {\n    children: stopTimes.map((stopTime, index) => {\n      const fishes = [maps.SPECTRAL_FISH_MAP[stopTime[0]], maps.GREEN_FISH_MAP[stopTime[0]]];\n      const blueFish = getBlueFish(stopTime);\n\n      if (blueFish !== null) {\n        fishes.push('hr', blueFish);\n      }\n\n      return /*#__PURE__*/_jsx(StopCard, {\n        index: index,\n        stopTime: stopTime,\n        children: /*#__PURE__*/_jsx(CardContent, {\n          children: /*#__PURE__*/_jsx(BaitList, {\n            baitGroups: fishes.map(fish => fish === 'hr' ? 'hr' : {\n              header: translate(locale, fish, 'name'),\n              baitGroupProps: getBaitGroup(fish)\n            })\n          })\n        })\n      }, stopTime);\n    })\n  });\n};\n\nexport default RouteInformationIntuition;","map":null,"metadata":{},"sourceType":"module"}