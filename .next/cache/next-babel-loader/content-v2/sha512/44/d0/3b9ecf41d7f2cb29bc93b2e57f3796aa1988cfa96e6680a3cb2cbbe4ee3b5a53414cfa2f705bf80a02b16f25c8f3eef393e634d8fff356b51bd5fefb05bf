{"ast":null,"code":"import { jsx as _jsx } from \"react/jsx-runtime\";\nimport _toConsumableArray from \"/root/ffxiv/node_modules/next/node_modules/@babel/runtime/helpers/esm/toConsumableArray\";\nimport React from 'react';\nimport CardContent from '@material-ui/core/CardContent';\nimport StopCardsContainer from './StopCardsContainer';\nimport StopCard from './StopCard';\nimport BaitList from './BaitList';\nimport { fishingSpots } from './ffxiv-ocean-fishing/data';\nimport * as maps from './maps';\nimport { getBaitGroup } from './utils';\nimport translate from '../translate';\nimport { useTranslation } from '../i18n';\n\nfunction getTimeSensitiveFishes(stopTime) {\n  var fishingSpot = maps.STOP_MAP[stopTime[0]];\n  var spectralFishingSpot = fishingSpots[fishingSpot.id + 1];\n  var time = stopTime[1];\n  return spectralFishingSpot.fishes.filter(function (fish) {\n    var fishTime = fish.spreadsheetData.time;\n    return fishTime !== null && fishTime.length < 3 && fishTime.includes(time);\n  });\n}\n\nvar RouteInformationTimeSensitive = function RouteInformationTimeSensitive(_ref) {\n  var stopTimes = _ref.stopTimes;\n\n  var _useTranslation = useTranslation('ocean-fishing'),\n      i18n = _useTranslation.i18n;\n\n  var locale = i18n.language;\n  return /*#__PURE__*/_jsx(StopCardsContainer, {\n    children: stopTimes.map(function (stopTime, index) {\n      var fishes = [maps.SPECTRAL_FISH_MAP[stopTime[0]]];\n      var timeFishes = getTimeSensitiveFishes(stopTime);\n\n      if (timeFishes.length > 0) {\n        fishes.push.apply(fishes, ['hr'].concat(_toConsumableArray(timeFishes)));\n      }\n\n      return /*#__PURE__*/_jsx(StopCard, {\n        index: index,\n        stopTime: stopTime,\n        children: /*#__PURE__*/_jsx(CardContent, {\n          children: /*#__PURE__*/_jsx(BaitList, {\n            baitGroups: fishes.map(function (fish) {\n              return fish === 'hr' ? 'hr' : {\n                header: translate(locale, fish, 'name'),\n                baitGroupProps: getBaitGroup(fish)\n              };\n            })\n          })\n        })\n      }, stopTime);\n    })\n  });\n};\n\nexport default RouteInformationTimeSensitive;","map":null,"metadata":{},"sourceType":"module"}