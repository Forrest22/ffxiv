{"ast":null,"code":"import { jsx as _jsx } from \"react/jsx-runtime\";\nimport React from 'react';\nimport clsx from 'clsx';\nimport { makeStyles } from '@material-ui/core/styles';\nimport IconButton from '@material-ui/core/IconButton';\nimport CheckIcon from '@material-ui/icons/Check';\nimport { ChecklistContext } from '../../pages/ocean-fishing/index';\nvar useStyles = makeStyles(function () {\n  return {\n    check: {\n      padding: 10\n    },\n    unchecked: {\n      opacity: 0.4\n    },\n    checked: {\n      color: 'green'\n    }\n  };\n});\n\nvar ChecklistCheckmark = function ChecklistCheckmark(_ref) {\n  var fishId = _ref.fishId;\n  var classes = useStyles();\n  return /*#__PURE__*/_jsx(ChecklistContext.Consumer, {\n    children: function children(_ref2) {\n      var checklist = _ref2.checklist,\n          setChecklist = _ref2.setChecklist;\n\n      var toggleFish = function toggleFish(fishId) {\n        if (checklist.includes(fishId)) {\n          var copy = checklist.slice();\n          copy.splice(copy.findIndex(function (id) {\n            return id === fishId;\n          }), 1);\n          setChecklist(copy);\n        } else {\n          var _copy = checklist.slice();\n\n          _copy.push(fishId);\n\n          _copy.sort(function (a, b) {\n            return a - b;\n          });\n\n          setChecklist(_copy);\n        }\n      };\n\n      return /*#__PURE__*/_jsx(IconButton, {\n        className: clsx(classes.check, checklist.includes(fishId) ? classes.checked : classes.unchecked),\n        onClick: toggleFish.bind(null, fishId),\n        children: /*#__PURE__*/_jsx(CheckIcon, {})\n      });\n    }\n  });\n};\n\nexport default ChecklistCheckmark;","map":null,"metadata":{},"sourceType":"module"}