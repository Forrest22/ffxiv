{"ast":null,"code":"import { jsx as _jsx } from \"react/jsx-runtime\";\nimport React from 'react';\nimport { makeStyles } from '@material-ui/core/styles';\nimport MuiPagination from '@material-ui/lab/Pagination';\nconst useStyles = makeStyles(theme => ({\n  pagination: {\n    display: 'flex',\n    justifyContent: 'center',\n    margin: theme.spacing(1)\n  }\n}));\n\nconst Pagination = ({\n  url,\n  data,\n  onChangeUrl\n}) => {\n  const classes = useStyles();\n\n  const handleChangePage = (_, page) => {\n    const parsedUrl = new URL(url, 'https://xivapi.com');\n    parsedUrl.searchParams.set('page', String(page));\n    onChangeUrl(page === 1 ? parsedUrl.pathname : `${parsedUrl.pathname}?${parsedUrl.searchParams.toString()}`);\n  };\n\n  if (!data || !data.Pagination) {\n    return null;\n  } else {\n    const pagination = data.Pagination;\n    return /*#__PURE__*/_jsx(\"div\", {\n      className: classes.pagination,\n      children: /*#__PURE__*/_jsx(MuiPagination, {\n        count: pagination.PageTotal,\n        defaultPage: pagination.Page,\n        onChange: handleChangePage\n      })\n    });\n  }\n};\n\nexport default Pagination;","map":null,"metadata":{},"sourceType":"module"}